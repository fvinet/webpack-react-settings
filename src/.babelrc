{
  "presets": [
    ["@babel/preset-env", {
      "corejs":  "2.6.11",
      "targets": [
        "last 2 versions",
        "not dead",
        "not < 2%",
        "not ie 11"
      ],
      "useBuiltIns": "entry"
    }], 
    "@babel/preset-react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "@babel/plugin-proposal-class-properties"
  ]
}